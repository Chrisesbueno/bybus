{
  "expo": {
    "name": "viajes",
    "slug": "viajes",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./src/utils/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/utils/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00B4D8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/405c0b8f-173f-4074-9680-cad80d10c052"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "runtimeVersion": "exposdk:48.0.0",
      "supportsTablet": true
    },
    "android": {
      "runtimeVersion": "exposdk:48.0.0",
      "package": "com.bybus.viajes",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/utils/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "favicon": "./src/utils/images/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "css"]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Espero no tengas nudes"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "405c0b8f-173f-4074-9680-cad80d10c052"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
