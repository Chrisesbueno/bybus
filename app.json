{
  "expo": {
    "name": "bybus-dev",
    "slug": "bybus-dev",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./src/utils/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/utils/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFCE48"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/03332bf0-024e-4dc4-a1bc-3c781c042c30"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.bybus",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/utils/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./src/utils/images/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Espero no tengas nudes"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "03332bf0-024e-4dc4-a1bc-3c781c042c30"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
